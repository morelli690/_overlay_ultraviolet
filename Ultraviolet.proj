<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0" DefaultTargets="BuildWindows;BuildLinux;BuildMacOS">
  <PropertyGroup>
    <IsMac>false</IsMac>
    <IsMac Condition="'$(OS)' == 'Unix' And Exists('/Library/Frameworks')">true</IsMac>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)' == ''">
    <Configuration>Debug</Configuration>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Platform)' == ''">
    <Platform>Any CPU</Platform>
  </PropertyGroup>
  <PropertyGroup Condition="'$(iOSPlatform)' == ''">
    <iOSPlatform>iPhoneSimulator</iOSPlatform>
  </PropertyGroup>
  <PropertyGroup Condition="'$(AndroidSdkDirectory)' == ''">
    <AndroidSdkDirectory>$(ANDROID_HOME)</AndroidSdkDirectory>
  </PropertyGroup>
  <PropertyGroup Condition="'$(JavaSdkDirectory)' == ''">
    <JavaSdkDirectory>$(JAVA_HOME)</JavaSdkDirectory>
  </PropertyGroup>

  <ItemGroup>
    <Ultraviolet_Windows_Solution Include="Source/Ultraviolet.Windows.sln">
      <Properties>Configuration=$(Configuration);Platform=$(Platform)</Properties>
    </Ultraviolet_Windows_Solution>
    <Ultraviolet_Desktop_Solution Include="Source/Ultraviolet.Desktop.sln">
      <Properties>Configuration=$(Configuration);Platform=$(Platform)</Properties>
    </Ultraviolet_Desktop_Solution>
    <Ultraviolet_MacOS_Solution Include="Source/Ultraviolet.macOS.sln">
      <Properties>Configuration=$(Configuration);Platform=$(Platform)</Properties>
    </Ultraviolet_MacOS_Solution>
    <Ultraviolet_MacOS_Bamboo_Solution Include="Source/Ultraviolet.MacOS.Bamboo.sln">
      <Properties>Configuration=$(Configuration);Platform=$(Platform)</Properties>
    </Ultraviolet_MacOS_Bamboo_Solution>
    <Ultraviolet_Android_Solution Include="Source/Ultraviolet.Android.sln">
      <Properties>Configuration=$(Configuration);Platform=$(Platform);AndroidSdkDirectory=$(AndroidSdkDirectory);JavaSdkDirectory=$(JavaSdkDirectory)</Properties>
    </Ultraviolet_Android_Solution>
    <Ultraviolet_iOS_Bamboo_Solution Include="Source/Ultraviolet.iOS.Bamboo.sln"/>
    <Ultraviolet_iOS_Solution Include="Source/Ultraviolet.iOS.sln"/>
    <Ultraviolet_NETCore_Solution Include="Source/Ultraviolet.NETCore.sln">
      <Properties>Configuration=$(Configuration);Platform=$(Platform)</Properties>
    </Ultraviolet_NETCore_Solution>
    <Ultraviolet_Tools_Solution Include="Source/Ultraviolet.Tools.sln">
      <Properties>Configuration=$(Configuration);Platform=$(Platform)</Properties>
    </Ultraviolet_Tools_Solution>
  </ItemGroup>
  
  <!-- DESKTOP -->	
  <Target Name="RestoreWindows">
    <Exec Condition="'$(OS)'=='Windows_NT'" 
      Command="&quot;Packages/nuget.exe&quot; restore &quot;@(Ultraviolet_Windows_Solution)&quot;"/>
    <Exec Condition="'$(OS)'=='Unix'" 
      Command="mono Packages/nuget.exe restore &quot;@(Ultraviolet_Windows_Solution)&quot;"/>
  </Target>
  <Target Name="RestoreLinux">
    <Exec Condition="'$(OS)'=='Windows_NT'" 
      Command="&quot;Packages/nuget.exe&quot; restore &quot;@(Ultraviolet_Desktop_Solution)&quot;"/>
    <Exec Condition="'$(OS)'=='Unix'" 
      Command="mono Packages/nuget.exe restore &quot;@(Ultraviolet_Desktop_Solution)&quot;"/>
  </Target>
  <Target Name="RestoreMacOS">
    <Exec Condition="'$(OS)'=='Windows_NT'" 
      Command="&quot;Packages/nuget.exe&quot; restore &quot;@(Ultraviolet_MacOS_Solution)&quot;"/>
    <Exec Condition="'$(OS)'=='Unix'" 
      Command="mono Packages/nuget.exe restore &quot;@(Ultraviolet_MacOS_Solution)&quot;"/>
  </Target>
  <Target Name="RestoreNETCore">
    <Exec Condition="'$(OS)'=='Windows_NT'" 
      Command="&quot;Packages/nuget.exe&quot; restore &quot;@(Ultraviolet_NETCore_Solution)&quot;"/>
    <Exec Condition="'$(OS)'=='Unix'" 
      Command="mono Packages/nuget.exe restore &quot;@(Ultraviolet_NETCore_Solution)&quot;"/>
  </Target>
  <Target Name="RestoreTools">
    <Exec Condition="'$(OS)'=='Windows_NT'" 
      Command="&quot;Packages/nuget.exe&quot; restore &quot;@(Ultraviolet_Tools_Solution)&quot;"/>
    <Exec Condition="'$(OS)'=='Unix'" 
      Command="mono Packages/nuget.exe restore &quot;@(Ultraviolet_Tools_Solution)&quot;"/>
  </Target>
  
  <Target Name="CleanWindows" Condition="'$(OS)' == 'Windows_NT'">
    <RemoveDir Directories="Binaries/AnyCPU"/>
    <MSBuild Projects="@(Ultraviolet_Windows_Solution)" Targets="Clean"/>
    <MSBuild Projects="@(Ultraviolet_Tools_Solution)" Targets="Clean"/>
  </Target>
  <Target Name="BuildWindows" Condition="'$(OS)' == 'Windows_NT'">
    <CallTarget Targets="RestoreWindows"/>
    <MSBuild Projects="@(Ultraviolet_Windows_Solution)"/>
    <MSBuild Projects="@(Ultraviolet_Tools_Solution)" Targets="Publish"/>
    <CallTarget Targets="CopyWindowsBinaries"/>
    <CallTarget Targets="CopyDesktopBinaries"/>
  </Target>
  <Target Name="CleanLinux" Condition="'$(OS)' == 'Unix' And '$(IsMac)' == 'false' ">
    <RemoveDir Directories="Binaries/AnyCPU"/>
    <MSBuild Projects="@(Ultraviolet_Desktop_Solution)" Targets="Clean"/>
    <MSBuild Projects="@(Ultraviolet_Tools_Solution)" Targets="Clean"/>
  </Target>
  <Target Name="BuildLinux" Condition="'$(OS)' == 'Unix' And '$(IsMac)' == 'false' ">
    <CallTarget Targets="RestoreLinux"/>
    <MSBuild Projects="@(Ultraviolet_Desktop_Solution)"/>
    <MSBuild Projects="@(Ultraviolet_Tools_Solution)" Targets="Publish"/>
    <CallTarget Targets="CopyDesktopBinaries"/>
  </Target>
  <Target Name="CleanMacOS" Condition="'$(OS)' == 'Unix' And '$(IsMac)' == 'true' ">
    <RemoveDir Directories="Binaries/AnyCPU"/>
    <MSBuild Projects="@(Ultraviolet_MacOS_Solution)" Targets="Clean"/>
    <MSBuild Projects="@(Ultraviolet_Tools_Solution)" Targets="Clean"/>
  </Target>
  <Target Name="BuildMacOS" Condition="'$(OS)' == 'Unix' And '$(IsMac)' == 'true' ">
    <CallTarget Targets="RestoreMacOS"/>
    <MSBuild Projects="@(Ultraviolet_MacOS_Solution)"/>
    <MSBuild Projects="@(Ultraviolet_Tools_Solution)" Targets="Publish"/>
    <CallTarget Targets="CopyMacOSModernBinaries"/>
    <CallTarget Targets="CopyMacOSBinaries"/>
    <CallTarget Targets="CopyDesktopBinaries"/>
  </Target>
  <Target Name="BuildMacOSBamboo" Condition="'$(OS)' == 'Unix' And '$(IsMac)' == 'true' ">
    <CallTarget Targets="RestoreMacOS"/>
    <MSBuild Projects="@(Ultraviolet_MacOS_Bamboo_Solution)"/>
    <CallTarget Targets="CopyMacOSModernBinaries"/>
    <CallTarget Targets="CopyMacOSBinaries"/>
    <CallTarget Targets="CopyDesktopBinaries"/>
  </Target>
  <Target Name="CleanNETCore">
    <RemoveDir Directories="Binaries/netstandard2.0"/>
    <MSBuild Projects="@(Ultraviolet_NETCore_Solution)" Targets="Clean"/>
  </Target>
  <Target Name="BuildNETCore">
    <CallTarget Targets="RestoreNETCore"/>
    <MSBuild Projects="@(Ultraviolet_NETCore_Solution)" Targets="Publish"/>
    <CallTarget Targets="CopyNETCoreBinaries"/>
  </Target>

